<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="esiea.org.app">

    <uses-permission android:name="android.permission.INTERNET" />



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SignUpActivity" />
        <activity android:name=".Activities.ClientActivity" />
        <activity android:name=".Activities.ConsultantActivity" />
        <activity android:name=".Activities.ProfilActivity" />
        <activity android:name=".Activities.ModifierProfilActivity" />

        <service
            android:name=".Service.DownloadSourceService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>